    <Action Name="Alarm" Id="{040003db-0126-4202-b8aa-167197bcc773}">
      <Implementation>
        <ST><![CDATA[//----机台状态提示---//
fbMachine[1].dwMessage[1].00			:=	fbMachine[1].xRunning;			//设备自动中
fbMachine[1].dwMessage[1].01		    :=	fbMachine[1].xStopped;			//设备停止状态
fbMachine[1].dwMessage[1].02			:=	fbMachine[1].xPaused;			//设备暂停状态
fbMachine[1].dwMessage[1].03			:=	fbMachine[1].xAlarm;			//设备报警中
fbMachine[1].dwMessage[1].04			:=	fbMachine[1].xIniting;			//设备初始化中
fbMachine[1].dwMessage[1].05			:=	fbMachine[1].xEmptying;			//设备清料中

//----机台报警信息----
IF NOT  fbMachine[1].xAirOn THEN
	fbMachine[1].dwAlarm[1].00:=TRUE; //[M01.00] 设备气压报警
	;
END_IF
(*
//安全门被打开1
IF fbMachine[1].xAutoMode AND NOT  strDoorBypassHmi  THEN
	IF	 NOT  fb_doors[1].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].02:=TRUE; //[M01.02] 设备安全门1被打开
	END_IF
	IF	 NOT  fb_doors[2].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].05:=TRUE; //[M01.05] 设备安全门2被打开
	END_IF
	IF	 NOT  fb_doors[3].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].06:=TRUE; //[M01.06] 设备安全门13被打开
	END_IF
	IF	 NOT  fb_doors[4].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].07:=TRUE; //[M01.07] 设备安全门4被打开
	END_IF
	IF	 NOT  fb_doors[5].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].08:=TRUE; //[M01.08] 设备安全门5被打开
	END_IF
	IF	 NOT  fb_doors[6].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].09:=TRUE; //[M01.09] 设备安全门6被打开
	END_IF
	IF	 NOT  fb_doors[7].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].10:=TRUE; //[M01.10] 设备安全门7被打开
	END_IF
	IF	 NOT  fb_doors[8].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].11:=TRUE; //[M01.11] 设备安全门8被打开
	END_IF
	IF	 NOT  fb_doors[9].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].12:=TRUE; //[M01.12] 设备安全门9被打开
	END_IF
	IF	 NOT  fb_doors[10].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].13:=TRUE; //[M01.13] 设备安全门10被打开
	END_IF
	IF	 NOT  fb_doors[11].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].14:=TRUE; //[M01.14] 设备安全门11被打开
	END_IF
	IF	 NOT  fb_doors[12].xIDoorClosed  THEN
		fbMachine[1].dwAlarm[1].15:=TRUE; //[M01.15] 设备安全门12被打开
	END_IF
END_IF

*)


//急停被按下
IF NOT IxSafetyRelayOK THEN 
	fbMachine[1].dwAlarm[1].03:=TRUE; //[M01.03] 设备急停被按下
END_IF


(*
tTimeoutAlarm(IN:= sPC_To_PLC.xPCAlive=0 AND (S03_LidExtracting.station.xRunning OR S03_LidExtracting.station.xIniting OR 
											 S05_Marking.station.xRunning OR   S05_Marking.station.xIniting   OR
											S06_CameraTesting.station.xRunning OR S06_CameraTesting.station.xIniting    )	, PT:=T#5S, Q=> , ET=> );
IF  tTimeoutAlarm.Q THEN
	fbMachine[1].dwAlarm[1].04:=TRUE; //[M01.04] PC无心跳
END_IF


*)


(*
//将上位机的报警读取在PLC中报警
sysMEMCPY(ADR(fbMachine[1].dwAlarm[2]),ADR(sPC_TO_PLC.ariPCAlarm[1]),4);
*)
(*EtherCAT总线报警*)	
fbEthercatTerminalState(
	xIActive:=TRUE ,
	xIReset:=fbMachine[1].xReset,
	axIState:= ,
	xQError=> ,
	iQErrorTerm=> );
IF   fbEthercatTerminalState.xQError THEN
 	fbMachine[1].dwAlarm[1].17:=TRUE; //[M01.15] PLC EL模块通讯报错，请检查EtherCAT总线状态	
END_IF
	
	
//----工站报警汇总----

xAlarm:= FALSE;							//先复位
FOR i :=1 TO gc_uiStationNum DO 
  IF fbStation[i].xAlarm  OR  fbEthercatTerminalState.xQError THEN		//整机故障单独 
    xAlarm:=TRUE; 
  END_IF
END_FOR

//----机台报警----
fbMachine[1].xAlarm:=
  fbMachine[1].dwAlarm[1]<>0
	OR fbMachine[1].dwAlarm[2]<>0
	OR fbMachine[1].dwAlarm[3]<>0
	OR fbMachine[1].dwAlarm[4]<>0
	OR fbMachine[1].dwAlarm[5]<>0
	OR xAlarm;





	
	





























































]]></ST>
      </Implementation>
    </Action>
