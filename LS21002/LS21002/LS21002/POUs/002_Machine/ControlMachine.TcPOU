    <Declaration><![CDATA[PROGRAM ControlMachine
VAR_input
	xAlarm                      	: BOOL;
	tonEmptyDone	: TON;					//Empty Done Delay
	bEmptyDone                  	: BOOL ;
	
	
END_VAR
VAR
	i,j : INT ;
	_xMaterialOnFlag : BOOL ;//环形线
	
	
	tTimeoutAlarm: TON;
	tCounterR_TRIG: R_TRIG;
	tCounter1R_TRIG: R_TRIG;
	tCounter2R_TRIG: R_TRIG;
	tPCAlarmR_TRIG: R_TRIG;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[A_Input();
Safety();
Waiting();
Timeout();
Alarm();
CycleTime();
Tip();
InitDone();
EmptyDone();
General();
Z_Output();

]]></ST>
